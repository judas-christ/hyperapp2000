{
  "compilerOptions": {
    "outDir": "public",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": ["./src/**/*.tsx", "./src/**/*.ts", "./src/**/*.svg"],
  "exclude": ["./src/**/*.test.ts", "./src/**/*.test.tsx"]
}
